@model Employee

@{ 
      ViewData["Title"] = "Create Record";
}

    <form asp-action="Create">
        <div class="form-group">
            <label class="custom-control-label">Name</label>
            <input asp-for="Emp_name" class="form-control" />
        </div>
        <div class="form-group">
            <label class="custom-control-label">City</label>
            <input asp-for="City" class="form-control" />
        </div>
        <div class="form-group">
            <label class="custom-control-label">Country</label>
            <input asp-for="Country" class="form-control" />
        </div>
        <div class="form-group">
            <label class="custom-control-label">Department</label>
            <input asp-for="Department" class="form-control" />
        </div>

        <div class="form-group">
            <input type="submit" value="submit" class="btn btn-primary"  />
        </div>
    </form>

@{ 
    if(@TempData["msg"]!=null)
    {
        <script>
            alert('@TempData["msg"]')
        </script>
    }
}